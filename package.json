{
  "name": "badminton",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "check": "tsc --noEmit",
    "i18n:types": "i18next-resources-for-ts interface -i src/i18n/locales/en -o ./@types/resources.d.ts",
    "dr:build": "pnpm dlx declarative-routing build && node scripts/patch-generated-routes.mjs",
    "dr:build:watch": "pnpm dlx declarative-routing build --watch",
    "dr:patch:watch": "node scripts/patch-generated-routes.mjs --watch",
    "openapi": "pnpm run openapi:yaml && pnpm run openapi:html",
    "openapi:yaml": "ts-node ./src/routes/openapi.ts",
    "openapi:html": "pnpm dlx @redocly/cli build-docs openapi-docs.yml",
    "predev": "pnpm dr:build",
    "prebuild": "pnpm dr:build",
    "pretypecheck": "pnpm dr:build",
    "prisma:generate": "prisma generate",
    "prisma:validate": "prisma validate",
    "prisma:format": "prisma format",
    "prisma:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "7.2.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.6.0",
    "i18next": "^25.7.3",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "query-string": "^9.3.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-i18next": "^16.5.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^8.2.0",
    "@prisma/adapter-pg": "7.2.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "i18next-resources-for-ts": "^2.0.0",
    "prisma": "7.2.0",
    "prisma-zod-generator": "^2.1.2",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "yaml": "^2.8.2"
  }
}
